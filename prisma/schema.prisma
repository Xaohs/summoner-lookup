// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DB_URL")
}


model allData {
    id String @id @default(auto()) @map("_id") @db.ObjectId
    puuid String
    matchID String
}